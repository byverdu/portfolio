extends layout

block content
	include header

	- if (messages.info)
		.message.info
			span= messages.info
	- if (messages.error)
		.message.error
			span= messages.error

	.pageWidth
		section.content
			each web in data
				a(href=web.url, alt=web.id+" website")
					div(class="circle " +web.id)
						.hoverInfo Hover me !
						.title
							h5= web.title
						.techs
							ul
								each tech in web.techs
									li= tech
						div(class= web.id+"Tooltip")

	include footer
